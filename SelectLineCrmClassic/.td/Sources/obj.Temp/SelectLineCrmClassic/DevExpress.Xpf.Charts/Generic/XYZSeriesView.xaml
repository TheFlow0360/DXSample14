    </Style>
    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Charts.Point3DSeriesView}" TargetType="{x:Type local:Point3DSeriesView}">
        <Setter Property="dxcore_unique_xmlns:BlendHelper2.StyleInfo" Value="DevExpress.Xpf.Charts\Generic\XYZSeriesView.xaml;13;6" /><Setter Property="LegendMarkerTemplate" Value="{StaticResource ResourceKey=MarkerSeriesLegendMarkerTemplate}" />
    </Style>
    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Charts.Bubble3DSeriesView}" TargetType="{x:Type local:Bubble3DSeriesView}">
