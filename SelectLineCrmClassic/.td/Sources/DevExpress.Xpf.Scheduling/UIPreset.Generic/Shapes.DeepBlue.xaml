<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">

    <Style x:Key="UIShape.Up.Base" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 4,0 8,4 0,4 4,0 Z " />
        <Setter Property="Height" Value="4" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="Margin" Value="0,0,0,1" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="8" />
    </Style>

    <Style x:Key="UIShape.Down.Base" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 0,0 8,0 4,4 0,0 Z " />
        <Setter Property="Height" Value="4" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="Margin" Value="0,1,0,0" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="8" />
    </Style>

    <Style x:Key="UIShape.Left.Base" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 0,4 4,0 4,8 0,4 Z " />
        <Setter Property="Height" Value="8" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="Margin" Value="0,0,1,0" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="4" />
    </Style>

    <Style x:Key="UIShape.Right.Base" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 0,0 0,8 4,4 0,0 Z " />
        <Setter Property="Height" Value="8" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="Margin" Value="1,0,0,0" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="4" />
    </Style>

    <Style x:Key="UIShape.Left" TargetType="{x:Type Path}" BasedOn="{StaticResource UIShape.Left.Base}" />

    <Style x:Key="UIShape.Right" TargetType="{x:Type Path}" BasedOn="{StaticResource UIShape.Right.Base}" />

    <Style x:Key="UIShape.Up" TargetType="{x:Type Path}" BasedOn="{StaticResource UIShape.Up.Base}" />

    <Style x:Key="UIShape.Down" TargetType="{x:Type Path}" BasedOn="{StaticResource UIShape.Down.Base}" />

    <Style x:Key="UIShape.Navigation.Plus" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 0,3 3,3 3,0 5,0 5,3 8,3 8,5 5,5 5,8 3,8 3,5 0,5 0,3 Z " />
        <Setter Property="Height" Value="8" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="8" />
    </Style>

    <Style x:Key="UIShape.Navigation.Minus" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 0,3 8,3 8,5 0,5 Z " />
        <Setter Property="Height" Value="2" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="8" />
    </Style>

    <Style x:Key="UIShape.Navigation.First" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 1,4 5,0 5,8 1,4 Z M 5,4 9,0 9,8 5,4 Z M 0,0 1,0 1,8 0,8 Z " />
        <Setter Property="Height" Value="8" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="9" />
    </Style>

    <Style x:Key="UIShape.Navigation.PrevPage" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 0,4 4,0 4,8 0,4 Z M 4,4 8,0 8,8 4,4 Z " />
        <Setter Property="Height" Value="8" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="Margin" Value="0,0,1,0" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="8" />
    </Style>

    <Style x:Key="UIShape.Navigation.PrevItem" BasedOn="{StaticResource UIShape.Left.Base}" TargetType="{x:Type Path}" />

    <Style x:Key="UIShape.Navigation.NextItem" BasedOn="{StaticResource UIShape.Right.Base}" TargetType="{x:Type Path}" />

    <Style x:Key="UIShape.Navigation.NextPage" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 0,0 0,8 4,4 0,0 Z M 4,0 4,8 8,4 4,0 Z " />
        <Setter Property="Height" Value="8" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="Margin" Value="1,0,0,0" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="8" />
    </Style>

    <Style x:Key="UIShape.Navigation.Last" TargetType="{x:Type Path}">
        <Setter Property="Data" Value="F1 M 0,0 0,8 4,4 0,0 Z M 4,0 4,8 8,4 4,0 Z M 8,0 9,0 9,8 8,8 Z " />
        <Setter Property="Height" Value="8" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Stretch" Value="Fill" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Width" Value="9" />
    </Style>

</ResourceDictionary>